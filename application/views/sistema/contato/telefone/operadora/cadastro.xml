<?xml version="1.0" encoding="UTF-8"?>
<root>
    <main_content>
        <form action="sistema/contato/telefone/operadora" name="cadastro-operadora-telefone" not-permitted="update" field-identifier="id" edit="sistema/contato/telefone/operadora/cadastro/$id">
            <tab>
                <tab_panel title="Cadastro" id="cadastro" is-active="TRUE">
                    <field_text name="id" id="id" title="Codigo" size='{"medium":4,"large":2}' disabled="TRUE">
                    </field_text>
                    <field_text name="operadora" id="operadora" title="Operadora" size='{"medium":8,"large":10}' required="TRUE" autofocus="TRUE">
                    </field_text>
                </tab_panel>
            </tab>
            <form_button type="save" id="salvar"></form_button>
            <form_button type="new" id="novo"></form_button>
            <form_button type="save_close" id="salvar-fechar" url="sistema/contato/telefone/operadora/consulta"></form_button>
            <form_button type="close" id="fechar" url="sistema/contato/telefone/operadora/consulta"></form_button>
        </form>
    </main_content>
</root>
