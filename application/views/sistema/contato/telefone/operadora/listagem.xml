<?xml version="1.0" encoding="UTF-8"?>
<root>
    <main_content>
        <table_list name="operadora-telefone" url="sistema/contato/telefone/operadora" title="Consulta Operadora Telefônica">
            <tl_filter>
                <field_text id="operadora" name="operadora" title="Operadora"></field_text>
            </tl_filter>
            <tl_row>
                <tl_col_link id="id" name="id" title="Código" order="ASC" size='{small:min}' url="sistema/contato/telefone/operadora/cadastro/{[{id}]}"></tl_col_link>
                <tl_col id="operadora" name="operadora" title="Operadora" data-type="text"></tl_col>
            </tl_row>
            <tl_button type="new" id="novo" url="sistema/contato/telefone/operadora/cadastro"></tl_button>
            <tl_selection type="select" id="selecao">
                <tls_button type="delete" id="excluir"></tls_button>
            </tl_selection>
        </table_list>
    </main_content>
</root>
